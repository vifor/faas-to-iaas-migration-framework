TEST_JWT=
# K6 Load Testing Environment Variables
# Copy this file to .env and update with your actual values

# API Configuration
API_BASE_URL=https://5brhnloiod.execute-api.sa-east-1.amazonaws.com/main
NODE_ENV=development

# Admin API Configuration
ADMIN_API_KEY=your-admin-api-key-here

# Cognito Configuration for Store Authentication
COGNITO_REGION=sa-east-1
COGNITO_USER_POOL_ID=sa-east-1_XXXXXXXXX
COGNITO_CLIENT_ID=your-cognito-client-id

# Test User Credentials (create these users in your Cognito User Pool)
TEST_USER_1_USERNAME=testuser1@example.com
TEST_USER_1_PASSWORD=TempPassword123!

TEST_USER_2_USERNAME=testuser2@example.com
TEST_USER_2_PASSWORD=TempPassword123!

TEST_MANAGER_USERNAME=manager1@example.com
TEST_MANAGER_PASSWORD=TempPassword123!

# FaaS vs IaaS Comparison Configuration
FAAS_BASE_URL=https://your-api-gateway.execute-api.region.amazonaws.com/stage
FAAS_AUTH_URL=https://cognito-idp.region.amazonaws.com/
FAAS_CLIENT_ID=your_cognito_client_id
FAAS_USERNAME=your_username
FAAS_PASSWORD=your_password
FAAS_ADMIN_API_KEY=your_admin_api_key

# IaaS Environment Configuration
IAAS_BASE_URL=http://localhost:3000/api/v1
IAAS_EMAIL=your_email@domain.com
IAAS_PASSWORD=your_iaas_password

# Smoke Test Configuration (uses FaaS variables by default)
SMOKE_TEST_BASE_URL=https://your-api-gateway.execute-api.region.amazonaws.com/stage
SMOKE_TEST_API_KEY=your_admin_api_key

# Performance Expectations (optional overrides)
FAAS_P50_EXPECTED=2000
FAAS_P95_EXPECTED=4000
IAAS_P50_EXPECTED=100
IAAS_P95_EXPECTED=300

# Environment-specific URLs (optional)
STAGING_API_URL=https://staging-api.petstore.com
PRODUCTION_API_URL=https://api.petstore.com

# K6 Cloud Configuration (optional)
K6_CLOUD_PROJECT_ID=your-k6-cloud-project-id
K6_CLOUD_TOKEN=your-k6-cloud-token

# Test Environment Settings
# Set to 'development', 'staging', or 'production' for different thresholds
NODE_ENV=development

# Load Test Specific Configuration
# Adjust these for different test intensities
LOAD_TEST_MAX_VUS=25
LOAD_TEST_DURATION=5m
STRESS_TEST_MAX_VUS=50
SPIKE_TEST_MAX_VUS=100